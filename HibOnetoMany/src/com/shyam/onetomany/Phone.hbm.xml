<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="com.shyam.onetomany.Phone" table="PHONE">
	<meta attribute="class-description">
	This class contains the Phone details.
	</meta>
	<id name="phone_id" type="int" column="PHONE_ID">
		<generator class="identity"/>
	</id>
	<property name="phone_name" type="string" column="PHONE_NAME" not-null="true" />
	<property name="phone_model" type="string" column="PHONE_MODEL" not-null="true" />
	
		
</class>

</hibernate-mapping>